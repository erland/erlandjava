<entity id="company">
    <class>erland.webapp.companylist.entity.Company</class>
    <data id="storage">
        <class>erland.webapp.common.GenericEntityStorage</class>
        <name>sql</name>
    </data>
    <data id="sql">
        <datasource>jdbc/companylist</datasource>
        <table>companies</table>
        <fields>
            <field id="id">
                <column>id</column>
                <type>Integer</type>
                <identity>true</identity>
                <auto>true</auto>
            </field>
            <field id="name">
                <column>name</column>
                <type>String</type>
            </field>
            <field id="username">
                <column>username</column>
                <type>String</type>
            </field>
            <field id="owner">
                <column>owner</column>
                <type>String</type>
            </field>
        </fields>
        <queries>
            <query id="all">
                <querystring/>
            </query>
        </queries>
    </data>
</entity>
