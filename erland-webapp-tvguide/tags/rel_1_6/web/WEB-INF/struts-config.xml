            <forward name="success" path=".user.viewexclusions"/>
        </action>
        <action
            path="/guest/viewexclusions"
            type="erland.webapp.tvguide.act.exclusion.SearchExclusionsAction"
            name="searchExclusionFB">
            <forward name="success" path=".guest.viewexclusions"/>
        </action>



