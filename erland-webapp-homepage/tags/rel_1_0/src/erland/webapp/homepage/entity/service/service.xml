<entity id="service">
    <class>erland.webapp.homepage.entity.service.Service</class>
    <data id="storage">
        <class>erland.webapp.common.GenericEntityStorage</class>
        <name>sql</name>
    </data>
    <data id="sql">
        <datasource>jdbc/homepage</datasource>
        <table>services</table>
        <fields>
            <field id="id">
                <column>id</column>
                <type>Integer</type>
                <auto>true</auto>
                <identity>true</identity>
            </field>
            <field id="name">
                <column>name</column>
                <type>String</type>
            </field>
            <field id="nameEnglish">
                <column>name_en</column>
                <type>String</type>
            </field>
            <field id="serviceClass">
                <column>serviceclass</column>
                <type>String</type>
            </field>
            <field id="serviceData">
                <column>servicedata</column>
                <type>String</type>
            </field>
            <field id="customizedServiceData">
                <column>customizedservicedata</column>
                <type>String</type>
            </field>
            <field id="transformerClass">
                <column>transformerclass</column>
                <type>String</type>
            </field>
            <field id="transformerData">
                <column>transformerdata</column>
                <type>String</type>
            </field>
        </fields>
        <queries>
            <query id="all">
                <orderstring>order by id asc</orderstring>
                <querystring/>
            </query>
        </queries>
    </data>
</entity>
